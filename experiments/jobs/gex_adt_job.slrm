#!/bin/bash

#SBATCH --job-name=gex_adt_sweep
#SBATCH --array=1 # 500 jobs
#SBATCH --gres=gpu:1
#SBATCH --cores=8
#SBATCH --partition=a40,t4v2,rtx6000
#SBATCH --mem=32G
#SBATCH --time=10-00:00:00
#SBATCH --output=sweep_array_%A-%a.log

pwd; hostname; date

source ~/miniconda3/etc/profile.d/conda.sh
conda activate single_cell_env
cd ../scripts
wandb agent --entity team-single-cell --project gex_adt_sweep zwctbvrx # Replace with correct API key

date